{
  "name": "node-ts-starter",
  "version": "1.0.0",
  "description": "### Instruções",
  "main": "index.js",
  "scripts": {
    "start:dev": "yarn lint && nodemon --watch 'src/' --exec 'ts-node src/index.ts' -e ts",
    "lint": "./node_modules/.bin/eslint --ext .ts src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "kiq7",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/supertest": "^2.0.7",
    "express": "^4.16.4",
    "global": "^4.3.2",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.3"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-with-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0"
  }
}
